{
  "manifest_version": 2,

  "name": "Save Link Extension",
  "description": "Extension for saving interesting links",
  "version": "1.0",

  "browser_action": {
    "default_icon": "link.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}